{
  // ===== ПРИМЕР 1: Базовая конфигурация =====
  "RedbService_Basic": {
    "DefaultCheckPermissionsOnLoad": false,
    "DefaultCheckPermissionsOnSave": false,
    "DefaultCheckPermissionsOnDelete": true,
    "DefaultLoadDepth": 10,
    "EnableSchemaMetadataCache": true,
    "SchemaMetadataCacheLifetimeMinutes": 30,
    "JsonOptions": {
      "WriteIndented": false,
      "UseUnsafeRelaxedJsonEscaping": true
    }
  },

  // ===== ПРИМЕР 2: Использование профиля =====
  "RedbService_WithProfile": {
    "Profile": "Development",
    "Overrides": {
      "DefaultLoadDepth": 15,
      "EnableSchemaMetadataCache": false,
      "JsonOptions": {
        "WriteIndented": true
      }
    }
  },

  // ===== ПРИМЕР 3: Продакшн конфигурация =====
  "RedbService_Production": {
    "Profile": "Production",
    "Overrides": {
      "DefaultLoadDepth": 3,
      "SchemaMetadataCacheLifetimeMinutes": 120,
      "SystemUserId": 1
    }
  },

  // ===== ПРИМЕР 4: Полная конфигурация =====
  "RedbService_Full": {
    // Стратегии обработки объектов
    "IdResetStrategy": "AutoCreateNewOnSave",
    "MissingObjectStrategy": "AutoSwitchToInsert",

    // Настройки безопасности
    "DefaultCheckPermissionsOnLoad": false,
    "DefaultCheckPermissionsOnSave": false,
    "DefaultCheckPermissionsOnDelete": true,
    "DefaultSecurityPriority": "Context",
    "SystemUserId": 0,

    // Настройки схем
    "DefaultStrictDeleteExtra": true,
    "AutoSyncSchemesOnSave": true,

    // Настройки загрузки
    "DefaultLoadDepth": 10,
    "DefaultMaxTreeDepth": 50,

    // Настройки производительности
    "EnableSchemaMetadataCache": true,
    "SchemaMetadataCacheLifetimeMinutes": 30,

    // Настройки валидации
    "EnableSchemaValidation": true,
    "EnableDataValidation": true,

    // Настройки аудита
    "AutoSetModifyDate": true,
    "AutoRecomputeHash": true,

    // Настройки JSON
    "JsonOptions": {
      "WriteIndented": false,
      "UseUnsafeRelaxedJsonEscaping": true
    }
  },

  // ===== ПРИМЕР 5: Конфигурация для разработки =====
  "RedbService_Development": {
    "Profile": "Development",
    "Overrides": {
      // Отключаем кеш для актуальности данных
      "EnableSchemaMetadataCache": false,
      
      // Подробное JSON для отладки
      "JsonOptions": {
        "WriteIndented": true
      },
      
      // Больше глубины для полной картины
      "DefaultLoadDepth": 20,
      "DefaultMaxTreeDepth": 100
    }
  },

  // ===== ПРИМЕР 6: Конфигурация для высокой производительности =====
  "RedbService_HighPerformance": {
    "Profile": "HighPerformance",
    "Overrides": {
      // Минимальная глубина
      "DefaultLoadDepth": 2,
      "DefaultMaxTreeDepth": 5,
      
      // Агрессивное кеширование
      "SchemaMetadataCacheLifetimeMinutes": 180,
      
      // Отключаем тяжелые операции
      "AutoRecomputeHash": false
    }
  },

  // ===== ПРИМЕР 7: Конфигурация для массовых операций =====
  "RedbService_BulkOperations": {
    "Profile": "BulkOperations",
    "Overrides": {
      // Минимальные настройки для максимальной скорости
      "DefaultLoadDepth": 1,
      "DefaultMaxTreeDepth": 1,
      "EnableSchemaValidation": false,
      "EnableDataValidation": false,
      "AutoSetModifyDate": false,
      "AutoRecomputeHash": false
    }
  },

  // ===== ПРИМЕР 8: Конфигурация для тестирования =====
  "RedbService_Testing": {
    "Profile": "IntegrationTesting",
    "Overrides": {
      // Отключаем кеш для изоляции тестов
      "EnableSchemaMetadataCache": false,
      
      // Подробное JSON для анализа
      "JsonOptions": {
        "WriteIndented": true
      },
      
      // Строгая обработка ошибок
      "IdResetStrategy": "Manual",
      "MissingObjectStrategy": "ThrowException"
    }
  },

  // ===== ПРИМЕР 9: Конфигурация для миграции данных =====
  "RedbService_DataMigration": {
    "Profile": "DataMigration",
    "Overrides": {
      // Максимальная толерантность к ошибкам
      "IdResetStrategy": "AutoCreateNewOnSave",
      "MissingObjectStrategy": "AutoSwitchToInsert",
      
      // Отключаем валидацию данных (могут быть некорректные)
      "EnableDataValidation": false,
      
      // Не удаляем поля при миграции
      "DefaultStrictDeleteExtra": false,
      
      // Системный приоритет
      "DefaultSecurityPriority": "System"
    }
  },

  // ===== ПРИМЕР 10: Конфигурация с переменными окружения =====
  // Эти значения можно переопределить через environment variables:
  // REDBSERVICE__DEFAULTCHECKPERMISSIONSONLOAD=true
  // REDBSERVICE__DEFAULTLOADDEPTH=5
  // REDBSERVICE__ENABLESCHEMAMETADATACACHE=false
  "RedbService_WithEnvOverrides": {
    "DefaultCheckPermissionsOnLoad": false,  // Можно переопределить через REDBSERVICE__DEFAULTCHECKPERMISSIONSONLOAD
    "DefaultLoadDepth": 10,                  // Можно переопределить через REDBSERVICE__DEFAULTLOADDEPTH
    "EnableSchemaMetadataCache": true        // Можно переопределить через REDBSERVICE__ENABLESCHEMAMETADATACACHE
  }
}
